{
  "name": "skeleton-react-redux-bootstrap",
  "version": "1.3.0",
  "description": "Simple project skeleton for React-redux based client side apps with Bootstrap styles.  (React Bootstrap sold separately.)",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register",
    "build": "cross-env NODE_ENV=production gulp",
    "build-debug": "cross-env NODE_ENV=debug gulp",
    "watch": "cross-env NODE_ENV=debug gulp watch"
  },
  "keywords": [
    "react",
    "redux",
    "reactredux",
    "bootstrap"
  ],
  "author": "Joseph Sikorski <joedski@gmail.com>",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "modules": true
      }
    },
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "commonjs": true
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "cross-env": "^2.0.0",
    "cssnano": "^3.7.4",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "gulp": "^3.9.1",
    "gulp-less": "^3.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "mocha": "^2.5.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  }
}
